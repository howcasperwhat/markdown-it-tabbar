{
  "name": "markdown-it-tabbar",
  "type": "module",
  "version": "0.1.1",
  "description": "A markdown-it plugin to group anything using tabbar.",
  "author": "casper",
  "license": "MIT",
  "homepage": "https://github.com/howcasperwhat/markdown-it-tabbar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howcasperwhat/markdown-it-tabbar.git"
  },
  "bugs": "https://github.com/howcasperwhat/markdown-it-tabbar/issues",
  "keywords": [
    "markdown",
    "markdown-it",
    "tabbar",
    "group",
    "tab"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles/*": "./styles/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "test": "vitest run",
    "serve": "serve -l 3000",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "markdown-it": ">= 13.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.14.9",
    "eslint": "^9.5.0",
    "markdown-it": "^14.1.0",
    "serve": "^14.2.4",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vitest": "^3.0.9"
  }
}
